<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.star.shade"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="24" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.star.shade.WelcomeActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.star.shade.MainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.star.shade.ShadeOption"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.star.shade.ApplianceList"
            android:label="@string/title_activity_appliance" >
        </activity>
    </application>

</manifest>